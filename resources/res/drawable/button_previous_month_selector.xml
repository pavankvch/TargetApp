<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_focused="true" android:state_pressed="false">
        <rotate android:drawable="@drawable/ic_arrow_up_blue_soft" android:fromDegrees="-90" android:toDegrees="-90" android:pivotX="50%" android:pivotY="50%"/>
    </item>
    <item android:state_focused="false" android:state_pressed="true">
        <rotate android:drawable="@drawable/ic_arrow_up_blue" android:fromDegrees="-90" android:toDegrees="-90" android:pivotX="50%" android:pivotY="50%"/>
    </item>
    <item android:state_focused="true" android:state_pressed="true">
        <rotate android:drawable="@drawable/ic_arrow_up_blue" android:fromDegrees="-90" android:toDegrees="-90" android:pivotX="50%" android:pivotY="50%"/>
    </item>
    <item>
        <rotate android:drawable="@drawable/ic_arrow_up_blue_soft" android:fromDegrees="-90" android:toDegrees="-90" android:pivotX="50%" android:pivotY="50%"/>
    </item>
</selector>
